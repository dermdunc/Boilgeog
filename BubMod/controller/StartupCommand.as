package BubMod.controller {	import BubMod.utilities.*;	import org.puremvc.as3.patterns.command.MacroCommand;		public class StartupCommand extends MacroCommand    {        /**         * Add the Subcommands to startup the PureMVC apparatus.         *          * Generally, it is best to prep the Model (mostly registering          * proxies)followed by preparation of the View (mostly registering          * Mediators).         */        override protected function initializeMacroCommand():void            {			//Prepares the View and Model and then calls the create schema command			this.addSubCommand( PrepModelCommand );            this.addSubCommand( PrepViewCommand );			this.addSubCommand( CreateSchemaCommand );        }    }}