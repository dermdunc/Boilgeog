package BubMod.controller {		import BubMod.view.*;	import BubMod.model.*;	import org.puremvc.as3.interfaces.ICommand;    import org.puremvc.as3.interfaces.INotification;    import org.puremvc.as3.patterns.command.SimpleCommand;		public class AddMenuToStage extends SimpleCommand implements ICommand {				override public function execute(note:INotification):void {			//Retrieve the menu data proxy			var menuDataProxy:MenuDataProxy = facade.retrieveProxy(MenuDataProxy.NAME) as MenuDataProxy;			//Create a new menu mask object. This has the main menu embedded			var menu:menuMask = new menuMask();			//Register a new mediator for the menu			facade.registerMediator( new MenuMediator(menu) );			//Tell the stagemediator to add the menu to the stage			sendNotification(ApplicationFacade.STAGE_ADD_MENU, menu);		}	}	}