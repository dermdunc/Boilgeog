package BubMod.controller {		import flash.display.*;	import BubMod.model.*;	import BubMod.view.*;	import BubMod.view.components.*;	import org.puremvc.as3.interfaces.ICommand;    import org.puremvc.as3.interfaces.INotification;    import org.puremvc.as3.patterns.command.SimpleCommand;		public class CreateEntityCommand extends SimpleCommand implements ICommand  {				override public function execute(note:INotification):void {			//Retrieve the schema data proxy			var schemaDataProxy:SchemaDataProxy = facade.retrieveProxy(SchemaDataProxy.NAME) as SchemaDataProxy;			//Retrieve the entity movieclip from the notification			var entityMC:EntityMC = note.getBody() as EntityMC;			//Add the entity to the entity array in the schema data proxy			schemaDataProxy.addEntity(entityMC);			//Register a mediator for the entity			facade.registerMediator( new EntityMediator(entityMC) );		}	}	}