package BubMod.view.components {		import flash.display.Sprite;	import flash.text.*;		public class Names extends Sprite {				// Constants:		// Public Properties:		// Private Properties:		private var lastX:Number = 0;		private var tableWidth = 20;		private var tableBox:Sprite		public var nameSpecified:String;		public var txtSize:Number;		private var textfield:TextField			// Initialization:		public function Names() { 					tableBox = new Sprite();			// drawing a white rectangle            tableBox.graphics.beginFill(0x33CC99); // white            tableBox.graphics.drawRect(0,0,300,100); // x, y, width, height            tableBox.graphics.endFill();			//var arialFont:ArialFont = new ArialFont();			var attrFormat:TextFormat = new TextFormat();			attrFormat.size = 60;			attrFormat.align = TextFormatAlign.CENTER;			attrFormat.color = 0xFFFFFF;			textfield = new TextField();		    textfield.wordWrap = true;            //textfield.text = nameSpecified;			textfield.width = 200;			//textfield.height = 100;			//textfield.textColor = nameColor;			textfield.setTextFormat(attrFormat);			//textfield.embedFonts = true;            this.addChild(tableBox);               this.addChild(textfield);		}				public function setTableName(specifiedTxt:String) {			textfield.text = specifiedTxt;		}			// Public Methods:		// Protected Methods:	}	}