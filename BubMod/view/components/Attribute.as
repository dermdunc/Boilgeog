package BubMod.view.components {		public class Attribute {				// Constants:		// Public Properties:		// Private Properties:		private var _attributeName:String = "";		private var _primaryKey:Boolean = false;		private var _foreignKey:ForeignKey;		private var _id:int = 0;			// Initialization:		public function Attribute(id:int) { 			_foreignKey = new ForeignKey();			_id = id;		}			// Public Methods:		//Constructor to get and set the attribute name		public function getAttributeName():String {			return _attributeName;		}				public function setAttributeName(attributeName:String):void {			_attributeName = attributeName;		}				public function getAttributeId():int {			return _id;		}				//Constructor to get and set the primary key		public function getPrimaryKey():Boolean {			return _primaryKey;		}				public function setPrimaryKey(primaryKey:Boolean):void {			_primaryKey = primaryKey;		}				//Constructor to get and set the foreign key		public function getForeignKey():ForeignKey {			return _foreignKey;		}				public function setForeignKey(foreignKey:ForeignKey):void {			_foreignKey = foreignKey;		}		// Protected Methods:	}	}