package BubMod.view.components {		import flash.display.*;		public class ForeignKey {				// Private Properties:		private var _isAForeignKey:Boolean = false;		private var _relatedTable:String = "";		private var _relatedKey:String = "";		// Public Properties:		public var connectionLine:MovieClip;			// Initialization:		public function ForeignKey() { 			//Create a new movieclip for the relationship			connectionLine = new MovieClip();			connectionLine.graphics.lineStyle(2.5, 0x999999, 100);		}			// Public Methods:		//Constructors to get and set the foreign key status		public function getForeignKeyStatus():Boolean {			return _isAForeignKey;		}				public function setForeignKeyStatus(foreignKeyStatus:Boolean):void {			_isAForeignKey = foreignKeyStatus;		}				//Constructors to get and set the related table		public function getRelatedTable():String {			return _relatedTable;		}				public function setRelatedTable(relatedTable:String):void {			_relatedTable = relatedTable;		}				//Constructors to get and set the related key		public function getRelatedKey():String {			return _relatedKey;		}				public function setRelatedKey(relatedKey:String):void {			_relatedKey = relatedKey;		}				//Constructors to get and set the relationship name		public function getConnectionName():String {			return connectionLine.name;;		}				public function setConnectionName(connectionName:String):void {			connectionLine.name = connectionName;		}	}	}