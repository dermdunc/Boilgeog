package {	    import org.puremvc.as3.interfaces.IFacade;    import org.puremvc.as3.patterns.facade.Facade;	//import org.puremvc.patterns.observer.Notification;	import BubMod.controller.*;        public class ApplicationFacade extends Facade implements IFacade {		        // Notification name constants		public static const UPDATE:String			= "update"		public static const RENDER:String			= "render"        public static const STARTUP:String          = "startup";		public static const MOUSEDOWN:String        = "mouse down";		public static const MOUSEUPDATE:String      = "mouse update";		public static const ONENTERFRAME:String      = "on enter frame";		public static const CREATESCHEMA:String      = "create schema";		public static const SCHEMA_CREATED:String    = "schema created";		public static const STAGE_ADD_BUBBLE:String   = "add bubble";		public static const ENTITY_POSITION_CHANGED:String   = "bubble moved";		public static const CREATE_BUBBLES:String     = "create bubbles";		public static const ZSorter:String 		  = "false";		public static const SORT_DEPTHS:String    = "sort depths";		public static const SHOW_HIDE_MENU:String    = "show or hide menu";		public static const STAGE_ADD_MENU:String    = "add menu to stage";		public static const CREATE_MENU:String    = "create menu";		public static const CREATE_ENTITY:String  =  "create single entity";		public static const SAVE_SCHEMA:String		= "save schema";		public static const RETRIEVE_SCHEMA:String	= "retrieve schema";		public static const BUILD_RELATIONSHIPS:String = "build relationships";		public static const REMOVE_RELATIONSHIPS:String = "remove relationships";		public static const STAGE_REMOVE_BUBBLE:String = "remove bubble";		public static const ADD_TABLE:String = "add table";		public static const REMOVE_TABLE:String = "remove table";		public static const STOP_NAVIGATION:String = "turn off navigation";		public static const START_NAVIGATION:String = "turn on navigation";		public static const SHOW_MENU_BTN:String = "show menu btn";		public static const SHOW_ALERT:String = "show alert";		public static const LOAD_TABLES:String = "load tables in menu";		public static const DELETE_SCHEMA:String = "delete schema";		// Singleton ApplicationFacade Factory Method        public static function getInstance(): ApplicationFacade {                        if (instance == null) {				instance = new ApplicationFacade( );			}            return instance as ApplicationFacade;        }				// Broadcast the STARTUP Notification		public function startup(app:Object):void {		    			trace("#1 The Concrete Facade class called ApplicationFacade broadcasts a STARTUP Notification.\n");			sendNotification( STARTUP, app );		}		        // Register Commands with the Controller         override protected function initializeController():void         {            super.initializeController();                                    registerCommand(STARTUP, StartupCommand);			registerCommand(MOUSEDOWN, Transform3DTo2DCommand);			registerCommand(ONENTERFRAME, OnEnterFrameCommand);			registerCommand(CREATESCHEMA, CreateSchemaCommand);			registerCommand(CREATE_BUBBLES, AddBubblesToStageCommand);			registerCommand(CREATE_MENU, AddMenuToStage);			registerCommand(CREATE_ENTITY, CreateEntityCommand);			registerCommand(SAVE_SCHEMA, SaveSchemaCommand);			registerCommand(RETRIEVE_SCHEMA, GetSchemaCommand);			registerCommand(DELETE_SCHEMA, DeleteSchemaCommand);        }    }}